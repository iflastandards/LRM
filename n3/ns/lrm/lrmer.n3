<http://iflastandards.info/ns/lrm/lrmerR9i> a rdf:Property ;
    rdfs:label "distributes"@en ;
    reg:lexicalAlias <http://iflastandards.info/ns/lrm/lrmerdistributes> ;
    reg:name "distributes" ;
    reg:status <http://metadataregistry.org/uri/RegStatus/1001> ;
    rdfs:comment "The items can be made available through the traditional distribution processes for physical items, or by making electronic items available for download, streaming, etc."@en ;
    rdfs:domain <http://iflastandards.info/ns/lrm/lrmerE6> ;
    rdfs:isDefinedBy lrmer: ;
    rdfs:range <http://iflastandards.info/ns/lrm/lrmerE4> ;
