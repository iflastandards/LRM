    rdfs:range <http://iflastandards.info/ns/lrm/lrmerE9> ;
    rdfs:subPropertyOf <http://iflastandards.info/ns/lrm/lrmerR1> ;
    owl:inverseOf <http://iflastandards.info/ns/lrm/lrmerR14i> ;
    skos:definition "Relates an agent with a particular nomen that was assigned by this agent"@en .

<http://iflastandards.info/ns/lrm/lrmerR14i> a rdf:Property ;
    rdfs:label "was assigned by"@en ;
    reg:lexicalAlias <http://iflastandards.info/ns/lrm/lrmerwasAssignedBy> ;
    reg:name "wasAssignedBy" ;
    reg:status <http://metadataregistry.org/uri/RegStatus/1001> ;
    rdfs:comment "In the bibliographic context, nomen assignment is applied to the creation of subject terms, controlled access points, identifiers, etc."@en ;
    rdfs:domain <http://iflastandards.info/ns/lrm/lrmerE9> ;
    rdfs:isDefinedBy lrmer: ;
