<http://iflastandards.info/ns/lrm/lrmerR16> a rdf:Property ;
    rdfs:label "has part nomen"@en ;
    reg:lexicalAlias <http://iflastandards.info/ns/lrm/lrmerhasPartNomen> ;
    reg:name "hasPartNomen" ;
    reg:status <http://metadataregistry.org/uri/RegStatus/1001> ;
    rdfs:comment "The whole-part relationship for nomens is essential in handling the attributes of components of nomens constructed using pre-existing nomens, as such attributes as language may differ between the parts of a compound nomen."@en ;
    rdfs:domain <http://iflastandards.info/ns/lrm/lrmerE9> ;
    rdfs:isDefinedBy lrmer: ;
