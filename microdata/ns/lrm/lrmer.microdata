    <meta itemprop="http://www.w3.org/2004/02/skos/core#definition" content="Relates an expression to a class of users for which the expression is intended" />
    <link itemprop="http://www.w3.org/2000/01/rdf-schema#domain" href="http://iflastandards.info/ns/lrm/lrmerE3" />
    <link itemprop="http://www.w3.org/2000/01/rdf-schema#isDefinedBy" href="http://iflastandards.info/ns/lrm/lrmer/" />
