      <link itemprop="http://www.w3.org/2000/01/rdf-schema#subPropertyOf" href="http://iflastandards.info/ns/lrm/lrmerR1" />
      <link itemprop="http://metadataregistry.org/uri/profile/regap/lexicalAlias" href="http://iflastandards.info/ns/lrm/lrmerwasCreatedByExpression" />
      <meta itemprop="http://www.w3.org/2000/01/rdf-schema#label" content="was created by expression" />
      <link itemprop="http://metadataregistry.org/uri/profile/regap/status" href="http://metadataregistry.org/uri/RegStatus/1001" />
