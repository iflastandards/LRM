        <div rel="rdfs:domain" resource="http://iflastandards.info/ns/lrm/lrmerE2"></div>
        <div property="rdfs:label" xml:lang="en" content="is inspiration for"></div>
      </div>
    </div>
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
    <div rel="rdfs:isDefinedBy" resource="http://iflastandards.info/ns/lrm/lrmer/"></div>
    <div rel="rdfs:range" resource="http://iflastandards.info/ns/lrm/lrmerE2"></div>
  </div>
  <div typeof="rdf:Property" about="http://iflastandards.info/ns/lrm/lrmerR5">
    <div rel="rdfs:subPropertyOf" resource="http://iflastandards.info/ns/lrm/lrmerR1"></div>
    <div property="skos:definition" xml:lang="en" content="Relates a work to an agent responsible for the creation of the intellectual or artistic content"></div>
    <div rel="rdfs:domain" resource="http://iflastandards.info/ns/lrm/lrmerE2"></div>
    <div rel="owl:inverseOf">
      <div typeof="rdf:Property" about="http://iflastandards.info/ns/lrm/lrmerR5i">
        <div rel="rdfs:domain" resource="http://iflastandards.info/ns/lrm/lrmerE6"></div>
        <div rel="rdfs:isDefinedBy" resource="http://iflastandards.info/ns/lrm/lrmer/"></div>
        <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
        <div rel="reg:lexicalAlias" resource="http://iflastandards.info/ns/lrm/lrmercreatedWork"></div>
        <div rel="owl:inverseOf" resource="http://iflastandards.info/ns/lrm/lrmerR5"></div>
        <div property="skos:definition" xml:lang="en" content="Relates an agent to a work for which the agent is responsible for the creation of intellectual or artistic content"></div>
        <div rel="rdfs:range" resource="http://iflastandards.info/ns/lrm/lrmerE2"></div>
        <div rel="rdfs:subPropertyOf" resource="http://iflastandards.info/ns/lrm/lrmerR1"></div>
        <div property="reg:name" content="createdWork"></div>
