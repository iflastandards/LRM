    rdfs:isDefinedBy <http://iflastandards.info/ns/lrm/lrmer> ;
    rdfs:label "was modified by"@en ;
    rdfs:range <http://iflastandards.info/ns/lrm/lrmer/E6> ;
    rdfs:subPropertyOf <http://iflastandards.info/ns/lrm/lrmer/R1> ;
    owl:inverseOf <http://iflastandards.info/ns/lrm/lrmer/R11i> ;
    skos:definition "Relates an item to an agent that made changes to this particular item without creating a new manifestation"@en .

<http://iflastandards.info/ns/lrm/lrmer/R11i>
    reg:lexicalAlias <http://iflastandards.info/ns/lrm/lrmer/modified> ;
    reg:name "modified" ;
    reg:status <http://metadataregistry.org/uri/RegStatus/1001> ;
    a rdf:Property ;
    rdfs:comment "Examples include adding annotations, adding an ex-libris, removing pages, rebinding, restoration."@en ;
    rdfs:domain <http://iflastandards.info/ns/lrm/lrmer/E6> ;
    rdfs:isDefinedBy <http://iflastandards.info/ns/lrm/lrmer> ;
    rdfs:label "modified"@en ;
    rdfs:range <http://iflastandards.info/ns/lrm/lrmer/E5> ;
    rdfs:subPropertyOf <http://iflastandards.info/ns/lrm/lrmer/R1> ;
    owl:inverseOf <http://iflastandards.info/ns/lrm/lrmer/R11> ;
    skos:definition "Relates an agent to an item that the agent made changes to without creating a new manifestation"@en .

<http://iflastandards.info/ns/lrm/lrmer/R12>
    reg:lexicalAlias <http://iflastandards.info/ns/lrm/lrmer/hasAsSubject> ;
    reg:name "hasAsSubject" ;
    reg:status <http://metadataregistry.org/uri/RegStatus/1001> ;
    a rdf:Property ;
    rdfs:comment "The logical connection between a work and a related subject entity serves as the basis both for identifying the subject of an individual work and for ensuring that all works relevant to a given subject are linked to that subject."@en ;
    rdfs:domain <http://iflastandards.info/ns/lrm/lrmer/E2> ;
    rdfs:isDefinedBy <http://iflastandards.info/ns/lrm/lrmer> ;
    rdfs:label "has as subject"@en ;
    rdfs:range <http://iflastandards.info/ns/lrm/lrmer/E1> ;
    rdfs:subPropertyOf <http://iflastandards.info/ns/lrm/lrmer/R1> ;
    owl:inverseOf <http://iflastandards.info/ns/lrm/lrmer/R12i> ;
    skos:definition "Relates a work to its topic(s)"@en .

<http://iflastandards.info/ns/lrm/lrmer/R12i>
    reg:lexicalAlias <http://iflastandards.info/ns/lrm/lrmer/isSubjectOf> ;
    reg:name "isSubjectOf" ;
    reg:status <http://metadataregistry.org/uri/RegStatus/1001> ;
    a rdf:Property ;
    rdfs:comment "The logical connection between a work and a related subject entity serves as the basis both for identifying the subject of an individual work and for ensuring that all works relevant to a given subject are linked to that subject."@en ;
    rdfs:domain <http://iflastandards.info/ns/lrm/lrmer/E1> ;
    rdfs:isDefinedBy <http://iflastandards.info/ns/lrm/lrmer> ;
    rdfs:label "is subject of"@en ;
