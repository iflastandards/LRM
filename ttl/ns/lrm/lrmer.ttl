    a rdf:Property ;
    rdfs:comment "In this case, the particular item used as a source of reproduction is significant, either by its provenance or due to item-specific characteristics such as annotations or ownership markings. The process of reproduction always results in a new manifestation, even when only a single item was produced from that manifestation."@en ;
    rdfs:domain <http://iflastandards.info/ns/lrm/lrmer/E5> ;
    rdfs:isDefinedBy <http://iflastandards.info/ns/lrm/lrmer> ;
    rdfs:label "has reproduction item"@en ;
    rdfs:range <http://iflastandards.info/ns/lrm/lrmer/E4> ;
    rdfs:subPropertyOf <http://iflastandards.info/ns/lrm/lrmer/R1> ;
    owl:inverseOf <http://iflastandards.info/ns/lrm/lrmer/R28i> ;
